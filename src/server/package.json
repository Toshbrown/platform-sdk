{
  "name": "databox-sdk",
  "version": "1.0.0",
  "description": "databox sdk server",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon --ignore 'flow*.flow' --exec babel-node server.js",
    "pm": "pm2 start server.js --name editor --interpreter ./node_modules/.bin/babel-node",
    "start": "nodemon --ignore 'flow*.flow' --exec babel-node server.js",
    "dev": "nodemon --ignore 'flow*.flow' --exec babel-node server.js 9000",
    "build": "webpack --progress",
    "docker": "docker build --no-cache -t $npm_package_name .",
    "release": "docker tag $npm_package_name tlodge/$npm_package_name && docker push tlodge/$npm_package_name"
  },
  "author": "Tom Lodge",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.0",
    "connect-mongostore": "^0.1.4",
    "connect-redis": "^3.0.2",
    "dockerode": "^2.2.10",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "http": "0.0.0",
    "mongoose": "^4.5.0",
    "passport": "^0.3.2",
    "passport-github": "^1.1.0",
    "react-hot-loader": "^1.3.1",
    "superagent": "^2.0.0",
    "tar-stream": "^1.5.2"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "nodemon": "^1.9.2",
    "webpack": "^3.4.1",
    "webpack-node-externals": "^1.6.0"
  }
}
